{
  "name": "petpet-language",
  "publisher": "too-many-limits",
  "displayName": "PetPet Language",
  "description": "Syntax highlighting for PetPet",
  "version": "0.3.4",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "petpet",
        "aliases": [
          "PetPet",
          "petpet"
        ],
        "extensions": [
          ".petpet"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "petpet",
        "scopeName": "source.petpet",
        "path": "./syntaxes/petpet.tmLanguage.json"
      }
    ]
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/Moonlight-Maya/PetPet-Extension.git"
  }
}
